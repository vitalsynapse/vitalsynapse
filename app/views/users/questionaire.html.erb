    <div class="info">
      <h2> Thank you for signing up! Let's start with some exciting quiz </h2>

    </div>
    <%= nested_form_for(@user) do |f| %>
        <%= f.fields_for :results do |result| %>
        <h4> Q1. <%= @questions.first.description %> </h4><br>

              <%= check_box_tag "user[results_attributes][0][answer_id]", 1%> <%= @questions.first.answers.first.description %><br>
              <%= check_box_tag "user[results_attributes][1][answer_id]", 2%><%= @questions.first.answers.find(2).description %><br>
              <%= check_box_tag "user[results_attributes][2][answer_id]", 3%><%= @questions.first.answers.find(3).description %><br>
